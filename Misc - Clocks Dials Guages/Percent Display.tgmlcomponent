    <Bind Color="#009530" Fill="#D8DADD" Name="Value" Percent="100">
        <Expose ExposedAttribute="Color" Name="On Color"/>
        <Expose ExposedAttribute="Fill" Name="Off Fill"/>
        <Expose ExposedAttribute="Percent" Name="100% ="/>
        <Expose ExposedAttribute="Name" Name="Display Bind"/>
        <Script OnDocumentLoad="onLoad" OnSignalChange="onChange"><![CDATA[var comp;
var ton;
var offCol;
var onCol;

function onLoad(evt)
{
	comp=evt.getCurrentTarget().getParentNode();
	ton = parseFloat(evt.getCurrentTarget().getAttribute("Percent"));
	offCol = evt.getCurrentTarget().getAttribute("Fill")
	onCol = evt.getCurrentTarget().getAttribute("Color")
}

function onChange(evt)
{
	var val =evt.getValue();
	var newVal=0;
	var bindList = comp.getElementsByTagName("Curve");
	
	if (val <= ton && val >= 0){
		newVal = (val/ton)*100;
	}
	else{
		if (val > ton) newVal = ton;
	}
	comp.getChildByName("Display").setAttribute("Content", Math.round(newVal));
	
	newVal = Math.round(newVal/10)-1;

	for(var i=0; i < bindList.length;i++){
		if (newVal >= i){
			bindList.item(i).setAttribute("Fill", onCol);
		}
		else{
			bindList.item(i).setAttribute("Fill", offCol);
		}
	}
}]]></Script>
    </Bind>
</Component>
